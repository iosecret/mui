    "prepublishOnly": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/iosecret/mui/issues"
  },
  "devDependencies": {
    "@types/node": "^16.11.3",
    "@types/react": "^17.0.20",
    "glob": "^7.1.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "intersection-observer": "^0.12.0",
    "react": "^17.0.2"
  },
  "sideEffects": false
}
