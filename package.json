{
  "name": "@iosecret/mui",
  "version": "1.0.6",
  "description": "轻量级通用 ui 库",
  "keywords": [
    "react",
    "ui"
  ],
  "author": "水逆",
  "homepage": "https://github.com/iosecret/mui#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iosecret/mui.git"
  },
  "scripts": {
